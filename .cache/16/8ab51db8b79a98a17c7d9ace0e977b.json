left/right)\r\n\r\nbutton, .button {\r\n  padding: 0.875rem 1.375rem;\r\n  border-radius: 50px;\r\n  cursor: pointer;\r\n  background-color: $almostWhite; \r\n  border: 2px solid $mediumGray;\r\n  font-weight: 500;\r\n  font-family: \"Epilogue\";\r\n\r\n}\r\n\r\n//Visibility \r\n.hide-for-mobile {\r\n  @include breakpoint-down(medium) {\r\n    display: none;\r\n  }\r\n\r\n  //hide for mobile\r\n\r\n}\r\n\r\n.hide-for-desktop{\r\n  //hide for desktop\r\n  @include breakpoint-up(large) {\r\n    display:none;\r\n  }\r\n}","scss\\_variables.scss":"//Colors\r\n$almostWhite: hsl(0, 0%, 98%);\r\n$mediumGray: hsl(0, 0%, 41%);\r\n$kindaBlack: hsl(0, 0%, 8%);\r\n\r\n//Font Sizes\r\n$font-small: 0.9375rem; //15px\r\n$font-medium: 1.18rem; //19px\r\n$font-large: 2.125rem;","scss\\_mixins.scss":"@media (min-width: 64em) {\r\n\r\n}\r\n\r\n$breakpoints-up: (\"medium\": \"40em\", \"large\": \"64em\");\r\n$breakpoints-down: (\"small\":\"39.9em\", \"medium\": \"63.9em\");\r\n\r\n@mixin breakpoint-up ($size) {\r\n    @media(min-width: map-get($breakpoints-up, $size)) {\r\n        @content;\r\n    }\r\n}\r\n\r\n@mixin breakpoint-down ($size) {\r\n    @media(max-width: map-get($breakpoints-down, $size)) {\r\n        @content;\r\n    }\r\n}\r\n\r\n","scss\\_header.scss":"@use 'variables' as *;\r\n@use 'mixins' as *;\r\n\r\n.header{\r\n\r\n    //Hamburger open styles\r\n\r\n    &.open {\r\n        .header__menu{\r\n            >span:first-child{\r\n                transform: rotate(45deg);\r\n            }\r\n            >span:nth-child(2){\r\n                opacity: 0;\r\n            }\r\n            >span:last-child{\r\n                transform: rotate(-45deg);\r\n            }\r\n        }\r\n\r\n    }\r\n\r\n    .overlay{\r\n        display: none;\r\n        position: fixed;\r\n        top: 0px;\r\n        right: 15.0625rem;\r\n        bottom: 0px;\r\n        left: 0px;\r\n        background: $kindaBlack;\r\n        opacity: 0.8;\r\n    }\r\n    nav {\r\n        padding: 24px 15px 24px 15px;\r\n    }\r\n\r\n    &__logo{\r\n\r\n        img{\r\n            width: 5.5625rem;\r\n            height: 1.9375rem; \r\n        }\r\n    }\r\n\r\n    &__menu{//Mobile Menu\r\n        >span{\r\n            display: block;\r\n            width: 33px;\r\n            height: 3px;\r\n            background-color: $mediumGray;\r\n            transition: all 300ms ease-in-out;\r\n            transform-origin: 5px;\r\n\r\n            &:not(:last-child) {\r\n                margin-bottom: 5px;\r\n            }\r\n        }\r\n\r\n\r\n    }\r\n\r\n    &__links{\r\n        a {\r\n            font-size: $font-small;\r\n            transition: color 200ms ease-in-out;\r\n\r\n            &:not(:last-child) {\r\n                margin-right: 32px;\r\n            }\r\n\r\n            &:hover {\r\n                color: $kindaBlack;\r\n            }\r\n        }\r\n    }\r\n\r\n    .cta {\r\n        font-size: $font-small;\r\n\r\n        a{\r\n            margin-right: 40px;\r\n            transition: color 200ms ease-in-out;\r\n\r\n            &:hover {\r\n                color: $kindaBlack;\r\n            }\r\n        }\r\n\r\n    }\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\n/* \r\n.logo {\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n.nav__links {\r\n    list-style: none;\r\n    & li {\r\n        display: inline-block;\r\n        padding: 0px 1.25rem;\r\n\r\n        & a:hover{\r\n            transition: all 0.3s ease-in-out 0s;\r\n            color: $kindaBlack;\r\n        }\r\n    }\r\n}\r\n\r\n\r\n.dropdown img {\r\n    margin-left: 5px;\r\n\r\n}\r\n\r\n.cta__links {\r\n\r\n    margin-left:auto;\r\n    list-style: none;\r\n    & li {\r\n\r\n        display: inline-block;\r\n        padding: 0px 1.25rem;\r\n\r\n        & a:hover{\r\n            transition: all 0.3s ease-in-out 0s;\r\n            color: $kindaBlack;\r\n        }\r\n\r\n    }\r\n}\r\n\r\nbutton {\r\n    padding: .56rem 1.56rem;\r\n    border-color: $mediumGray;\r\n    border-radius: 1rem;\r\n    background-color: $almostWhite; \r\n\r\n    &:hover {\r\n        cursor: pointer;\r\n        transition: all 0.3s ease-in-out 0s;\r\n        color: $kindaBlack;\r\n        border-color: $kindaBlack;\r\n    }\r\n\r\n} */","scss\\_animations.scss":"@keyframes fade-in {\r\n    from{\r\n        display: none;\r\n        opacity: 0;\r\n    }\r\n    1% {\r\n        display: block;\r\n        opacity: 0;\r\n    }\r\n    to {\r\n        display: block;\r\n        opacity: 1;\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"b2b61146f9a05f433a9a9943f94013df","cacheData":{"env":{}}}