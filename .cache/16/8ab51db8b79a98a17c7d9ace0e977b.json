 1.18rem; //19px\r\n$font-large: 2.125rem;","scss\\_mixins.scss":"@media (min-width: 64em) {\r\n\r\n}\r\n\r\n$breakpoints-up: (\"medium\": \"40em\", \"large\": \"64em\");\r\n$breakpoints-down: (\"small\":\"39.9em\", \"medium\": \"63.9em\");\r\n\r\n@mixin breakpoint-up ($size) {\r\n    @media(min-width: map-get($breakpoints-up, $size)) {\r\n        @content;\r\n    }\r\n}\r\n\r\n@mixin breakpoint-down ($size) {\r\n    @media(max-width: map-get($breakpoints-down, $size)) {\r\n        @content;\r\n    }\r\n}\r\n\r\n","scss\\_header.scss":"@use 'variables' as *;\r\n@use 'mixins' as *;\r\n\r\n.header{\r\n\r\n    //Hamburger open styles\r\n\r\n    &.open {\r\n\r\n        .header__toggle{\r\n            >span:first-child{\r\n                transform: rotate(45deg);\r\n            }\r\n            >span:nth-child(2){\r\n                opacity: 0;\r\n            }\r\n            >span:last-child{\r\n                transform: rotate(-45deg);\r\n            }\r\n        }\r\n\r\n    }\r\n\r\n    .overlay{\r\n        visibility: hidden;\r\n        position: fixed;\r\n        top: 0px;\r\n        right: 0px;\r\n        bottom: 0px;\r\n        left: 0px;\r\n        width: 35%;\r\n        background: $kindaBlack;\r\n        opacity: 0.8;\r\n    }\r\n    nav {\r\n        padding: 24px 15px 24px 15px;\r\n    }\r\n\r\n    &__logo{\r\n\r\n        img{\r\n            width: 5.5625rem;\r\n            height: 1.9375rem; \r\n        }\r\n    }\r\n\r\n    &__toggle{//Mobile Menu Toggle\r\n        >span{\r\n            display: block;\r\n            width: 33px;\r\n            height: 3px;\r\n            background-color: $mediumGray;\r\n            transition: all 300ms ease-in-out;\r\n            transform-origin: 5px;\r\n\r\n            &:not(:last-child) {\r\n                margin-bottom: 5px;\r\n            }\r\n        }\r\n\r\n\r\n    }\r\n\r\n    &__menu{\r\n        display: block;\r\n        background: $almostWhite;\r\n        float: right;\r\n        width: 65%;\r\n        height: 100vh;\r\n        padding: 1.875rem;\r\n        top: 0px;\r\n        bottom: 0px;\r\n\r\n        a {\r\n            color: $mediumGray;\r\n            display: block;\r\n            padding: 0.625rem;\r\n        }\r\n    }\r\n\r\n    &__links{\r\n        a {\r\n            font-size: $font-small;\r\n            transition: color 200ms ease-in-out;\r\n\r\n            &:not(:last-child) {\r\n                margin-right: 32px;\r\n            }\r\n\r\n            &:hover {\r\n                color: $kindaBlack;\r\n            }\r\n        }\r\n    }\r\n\r\n    .cta {\r\n        font-size: $font-small;\r\n\r\n        a{\r\n            margin-right: 40px;\r\n            transition: color 200ms ease-in-out;\r\n\r\n            &:hover {\r\n                color: $kindaBlack;\r\n            }\r\n        }\r\n\r\n    }\r\n\r\n}\r\n\r\n.ctaMobile {\r\n    position: relative;\r\n    text-align: center;\r\n\r\n    a {\r\n        margin-bottom: 1rem;\r\n    }\r\n\r\n\r\n    &.button{\r\n        display: inline;\r\n        padding: 13px 69px\r\n\r\n    }\r\n\r\n}\r\n\r\n\r\n\r\n\r\n/* \r\n.logo {\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n.nav__links {\r\n    list-style: none;\r\n    & li {\r\n        display: inline-block;\r\n        padding: 0px 1.25rem;\r\n\r\n        & a:hover{\r\n            transition: all 0.3s ease-in-out 0s;\r\n            color: $kindaBlack;\r\n        }\r\n    }\r\n}\r\n\r\n\r\n.dropdown img {\r\n    margin-left: 5px;\r\n\r\n}\r\n\r\n.cta__links {\r\n\r\n    margin-left:auto;\r\n    list-style: none;\r\n    & li {\r\n\r\n        display: inline-block;\r\n        padding: 0px 1.25rem;\r\n\r\n        & a:hover{\r\n            transition: all 0.3s ease-in-out 0s;\r\n            color: $kindaBlack;\r\n        }\r\n\r\n    }\r\n}\r\n\r\nbutton {\r\n    padding: .56rem 1.56rem;\r\n    border-color: $mediumGray;\r\n    border-radius: 1rem;\r\n    background-color: $almostWhite; \r\n\r\n    &:hover {\r\n        cursor: pointer;\r\n        transition: all 0.3s ease-in-out 0s;\r\n        color: $kindaBlack;\r\n        border-color: $kindaBlack;\r\n    }\r\n\r\n} */","scss\\_animations.scss":"@keyframes fade-in {\r\n    from{\r\n        visibility: hidden;\r\n        opacity: 0;\r\n    }\r\n1% {\r\n    visibility: visible;\r\n    opacity: 0;\r\n}\r\n    to {\r\n        opacity: 0.8;\r\n        visibility: visible;\r\n    }\r\n}\r\n\r\n.fade-in {\r\n    animation: fade-in 300ms ease-in-out forwards;  //\"fade in\" was defined in the animations.scss. \"forwards\" makes it so that the animation stops at the \"to\" portion instead of reverting to the beginning of the animation cycle once it's completed.\r\n}\r\n\r\n\r\n@keyframes fade-out {\r\n    from{\r\n        visibility: visible;\r\n        opacity: 0.8;\r\n    }\r\n\r\n    99% {\r\n        visibility: visible;\r\n        opacity: 0;\r\n    }\r\n    to {\r\n        visibility: hidden;\r\n        opacity: 0;\r\n    }\r\n}\r\n\r\n.fade-out {\r\n    animation: fade-out 300ms ease-in-out forwards;\r\n}"},"lineCount":null}},"error":null,"hash":"1860229efa4d250eda29741192c5480d","cacheData":{"env":{}}}